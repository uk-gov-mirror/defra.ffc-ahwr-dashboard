    "@types/hapi__hapi": "^20.0.10",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "babel-jest": "^30.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "7.1.2",
    "dotenv": "^16.0.3",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "global-jsdom": "26.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^30.0.2",
    "jest-junit": "^16.0.0",
    "jest-when": "^3.7.0",
    "jsdom": "^26.0.0",
    "mini-css-extract-plugin": "^2.5.3",
